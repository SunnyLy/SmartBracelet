<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:startColor="#00333333"
                android:endColor="#00333333"
                android:angle="90.0"
                android:centerColor="#00dddddd" />
            <stroke
                android:width="1.0dip"
                android:color="#00333333" />
        </shape>
    </item>
    <item
        android:left="4.0dip"
        android:top="1.0dip"
        android:right="4.0dip"
        android:bottom="1.0dip">
        <shape android:shape="rectangle">
            <gradient
                android:startColor="#00aaaaaa"
                android:endColor="#00aaaaaa"
                android:angle="90.0"
                android:centerColor="#00ffffff" />
        </shape>
    </item>
</layer-list>